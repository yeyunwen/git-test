  "bugs": {
    "url": "https://github.com/yeyunwen/git-test/issues"
  },
  "homepage": "https://github.com/yeyunwen/git-test#readme",
  "devDependencies": {
    "commitizen": "^4.3.0",
    "eslint": "^8.52.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.0.2",
    "prettier": "^3.0.3"
  }
}
